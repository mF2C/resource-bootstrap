{
  "method": "jwt",
  "instance": "jwt",
  "name": "JWT Authentication",
  "description": "External Authentication with JWT",
  "token": "provide-valid-authentication-token",
  "acl": {
    "owner": {
      "principal": "ADMIN",
      "type": "ROLE"
    },
    "rules": [
      {
        "principal": "ADMIN",
        "type": "ROLE",
        "right": "ALL"
      },
      {
        "principal": "ANON",
        "type": "ROLE",
        "right": "VIEW"
      },
      {
        "principal": "USER",
        "type": "ROLE",
        "right": "VIEW"
      }
    ]
  }
}
